{
  "name": "@nestjs-packages/admin",
  "version": "0.0.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@admin)",
  "main": "dist/index.js",
  "author": "Sumin Choi <greatSumini@gmail.com> (https://sumini.dev)",
  "license": "MIT",
  "homepage": "https://github.com/nestjs-packages/admin#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp -r lib/public dist/public && cp -r lib/views dist/views",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "check": "run-p lint test:e2e",
    "release": "release-it"
  },
  "bugs": {
    "url": "https://github.com/nestjs-packages/admin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nestjs-packages/admin.git"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-angular": "19.7.1",
    "@nestjs/common": "8.4.1",
    "@nestjs/core": "8.4.1",
    "@types/express": "5.0.0",
    "@types/jest": "28.1.8",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "22.13.8",
    "@types/nunjucks": "3.2.6",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "husky": "9.1.7",
    "jest": "28.1.3",
    "npm-run-all2": "5.0.2",
    "prettier": "3.4.2",
    "reflect-metadata": "0.1.13",
    "release-it": "17.11.0",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "source-map-support": "0.5.21",
    "ts-jest": "28.0.8",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "nunjucks": "^3.2.3"
  }
}
