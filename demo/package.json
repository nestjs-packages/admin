{
  "name": "demo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "set": "docker compose -f docker-compose.yml up",
    "set:down": "docker-compose -f docker-compose.yml down --volumes && docker system prune",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "ts-node copy-public.ts",
    "start": "nest start",
    "start:dev": "npm run start -- --watch",
    "start:debug": "npm run start -- --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs-packages/admin": "file:..",
    "@nestjs/common": "8.4.1",
    "@nestjs/core": "8.4.1",
    "@nestjs/platform-express": "8.4.1",
    "@nestjs/typeorm": "8.0.2",
    "mysql2": "3.12.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "typeorm": "0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.5",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "8.4.1",
    "@types/express": "5.0.0",
    "@types/fs-extra": "11.0.4",
    "@types/node": "22.10.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "fs": "0.0.1-security",
    "fs-extra": "11.2.0",
    "prettier": "3.4.2",
    "source-map-support": "0.5.21",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
